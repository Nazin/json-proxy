{
  "name": "json-proxy",
  "version": "0.2.0",
  "description": "JSON proxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nazin/json-proxy.git"
  },
  "devDependencies": {
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  },
  "dependencies": {
    "ajv": "^6.0.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cpy-cli": "^1.0.1",
    "express": "^4.16.2",
    "formidable": "^1.1.1",
    "jsoneditor": "^5.13.1",
    "request": "^2.83.0"
  },
  "scripts": {
    "start": "npm run copy && npm run server",
    "server": "./node_modules/.bin/babel-node index.js",
    "copy": "./node_modules/.bin/cpy ./**/*.* ../../../public/jsoneditor --cwd=node_modules/jsoneditor/dist --parents",
    "qc": "npm run qc:js",
    "qc:js": "./node_modules/.bin/eslint server --quiet",
    "qc:js:fix": "./node_modules/.bin/eslint server --quiet --fix"
  },
