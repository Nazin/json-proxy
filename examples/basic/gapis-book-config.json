{
  "proxy": {
    "enabled": false
  },
  "endpoints": [
    {
      "url": "https://www.googleapis.com",
      "name": "gapis",
      "urls": [
        {
          "enabled": true,
          "url": "/books/v1/volumes",
          "method": "GET",
          "sendRQ": true,
          "rules": [
            {
              "condition": "isResponse",
              "actions": [
                "responseBody = loadFromFile('data/books-volumes.json')"
              ]
            }
          ]
        }
      ]
    }
  ]
}